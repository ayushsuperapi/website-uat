{
    "functions": {
      "pages/**/*.js": {
        "maxDuration": 30
      },
      "app/**/*.js": {
        "maxDuration": 30
      }
    },
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Accel-Buffering",
            "value": "no"
          },
          {
            "key": "X-Vercel-No-Cache",
            "value": "1"
          }
        ]
      }
    ]
  }