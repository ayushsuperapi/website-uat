{
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "https://platform.superapi.cloud/app",
      "headers": [
        {
          "key": "x-forwarded-host",
          "value": "uat.trysuperapi.com"
        },
        {
          "key": "origin",
          "value": "https://uat.trysuperapi.com"
        }
      ]
    },
    {
      "source": "/dashboard/(.*)",
      "destination": "https://platform.superapi.cloud/app/$1",
      "headers": [
        {
          "key": "x-forwarded-host",
          "value": "uat.trysuperapi.com"
        },
        {
          "key": "origin",
          "value": "https://uat.trysuperapi.com"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "destination": "https://platform.superapi.cloud/app/$1",
      "headers": [
        {
          "key": "x-forwarded-host",
          "value": "uat.trysuperapi.com"
        },
        {
          "key": "origin",
          "value": "https://uat.trysuperapi.com"
        }
      ]
    },
    {
      "source": "/app/(.*)",
      "destination": "https://platform.superapi.cloud/app/$1",
      "headers": [
        {
          "key": "x-forwarded-host",
          "value": "uat.trysuperapi.com"
        },
        {
          "key": "origin",
          "value": "https://uat.trysuperapi.com"
        }
      ]
    }
  ]
} 